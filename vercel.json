{
  "version": 2,
  "buildCommand": "./build.sh",
  "outputDirectory": "dist/moustachio",
  "installCommand": "npm install",
  "framework": "angular",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "src/app/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
